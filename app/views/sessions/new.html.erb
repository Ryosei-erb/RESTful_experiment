<% provide(:title,"Log in") %>
<h1>Log in</h1>

<div>
	<%= form_for(:session,url: login_url) do |f| %>
		<%= f.label :email %>
		<%= f.email_field :email %>
		
		<%= f.label :password %>
		<%= f.password_field :password %>
		
		<%= f.submit "Log in", class:"btn" %>
	<% end %>
	
	<p>New user? <%=link_to "Sign up ",signup_path %></p>
	
</div>