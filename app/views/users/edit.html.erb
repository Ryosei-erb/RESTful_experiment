<% provide(:title,"Edit user") %>
<h2 class="login">個人情報を編集</h2>

<div class="login">
	<%= form_for(@user) do|f| %>
	<%= render "shared/error_messages",object:f.object%>
	
	<%= f.label :name %>
	<%= f.text_field :name %>
	
	<%= f.label :email %>
	<%= f.email_field :email %>
	
	<%= f.label :password %>
	<%= f.password_field :password %>
	
	<%= f.label :password_confirmation ,"Password (again)" %>
	<%= f.password_field :password_cofirmation %>
	
	<%= f.submit "Save changes",class:"form" %>
	<% end %>
	
	
</div>